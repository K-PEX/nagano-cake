<h2 class="head-line title_h2 text-center">会員一覧</h2>
<div class="row">
  <div class="col-xs-10 col-xs-offset-1">
    <table class="table table-hover table-bordered table-condensed">
      <thead>
        <tr>
          <th>会員ID</th>
          <th>氏名</th>
          <th>メールアドレス</th>
          <th>ステータス</th>
        </tr>
      </thead>

      <tbody>
        <%#=@coustomers.each do |customer| %>
         <tr>
          <th>1<%#= customer.id %></th>
          <th><%= link_to "namae ", admin_costomers_show_path %></th>
           <td>email</td>
          <td><%# if customer.is_enabled == true %>
              有効
              <%# else %>
              退会
              <%# end %>
          </td>
        </tr>
      </tbody>
      <%#= paginate @kaminariのinstall %>
    </table>
  </div>

